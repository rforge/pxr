## The function is currently defined as
function (x, n = 3, c = "0") 
{
    if (is.numeric(x)) 
        x <- format(x, trim = T)
    x <- paste(paste(rep("0", n), sep = "", collapse = ""), x, 
        sep = "")
    nn <- nchar(x)
    x <- substr(x, nn - n + 1, nn)
    return(x)
  }
}
